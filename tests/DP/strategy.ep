Happy = { exec = function() puts("I'm smiling"); };
Angry = { exec = function() puts("Bastard!"); };

Mood = {
	new = function(mood) {
		m_mood = mood;
		
		m_exec = function(self) {
			self.mood.exec();
		};
		
		return {
			mood = m_mood,
			exec = m_exec
		};
	}
};

test = Mood.new(Happy);
test:exec();


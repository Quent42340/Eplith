v1, v2, v3, v4, v5 = 0, 1, 2, 3, 4;

gets(user);

set(v1, v2, v3, v4, v5) {
	(user == 0):(4, 3, 2, 1, 0)
	(user == 1):(3, 2, 1, 0, 4)
	(user == 2):(2, 1, 0, 4, 3)
	(user == 3):(1, 0, 4, 3, 2)
	(default):(same,same,same,same,0)
};


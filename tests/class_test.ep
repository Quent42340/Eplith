Op = {
	new = function(_a, _b) {
		a = _a;
		b = _b;
		
		add = function(self) {
			return self.a + self.b;
		};
		
		sub = function(self) {
			return self.a - self.b;
		};
		
		#print(a + " " + b + "\n");
		
		return {a = a,
				b = b,
				add = add,
				sub = sub};
	}
};

test = Op.new(5, 3);
print(test:add() + "\n");


// Brainfuck interpreter, author eRTI
#include <iostream>
#include <stack>

using namespace std;

int main()
{
	unsigned char b[30000] = {0}; char *k = "++++++++++[>+++++++>++++++++++>+++>+<<<<-]>++.>+.+++++++..+++.>++.<<+++++++++++++++.>.+++.------.--------.>+.>."; 
	unsigned char *x = b; stack<char *> y;
	while(*k)
	{
		if(*k == '+') ++*x;
		else if(*k == '-') --*x;
		else if(*k == '>') ++x;
		else if(*k == '<') --x;
		else if(*k == ',') *x = cin.get();
		else if(*k == '.') cout << *x;
		else if(*k == '[') y.push(k + 1);
		else if(*k == ']')
		{
			if(*x) { k = y.top(); continue; }
			else y.pop();
		}
		
		cout.flush();
		k++;
	}
	return 0;
}
